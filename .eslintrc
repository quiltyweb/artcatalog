{
  "extends": [
    "wesbos",
    "plugin:cypress/recommended"
  ],
  "rules": {
    "no-console": 2,
    "react/jsx-filename-extension": [1, { "extensions": [".tsx", ".ts"] }],
    "react/prop-types": [0],
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120
      }
    ],
    "jest/expect-expect": "off"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    }
  },
  "plugins": ["cypress"]
}